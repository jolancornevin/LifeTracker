---
arrowParens: always
bracketSpacing: true
jsxBracketSameLine: false
jsxSingleQuote: false
quoteProps: as-needed
singleQuote: true
semi: true
useTabs: true
printWidth: 80
tabWidth: 4
trailingComma: all
importOrder: ['^react$', '^react-native$', '^@', '^[a-zA-Z]', '^[./]']
importOrderSeparation: true
importOrderSortSpecifiers: true
